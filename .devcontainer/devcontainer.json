{
    "name": "Elixir Development",
    "dockerComposeFile": [
        "../.docker/docker-compose.yml"
    ],
    "service": "elixir-phenix",
    "workspaceFolder": "/app",
    "settings": {
        "[elixir]": {
            "editor.formatOnSave": true,
            "editor.defaultFormatter": "JakeBecker.elixir-ls"
          },
    },
    "extensions": [
        "JakeBecker.elixir-ls",
        "saratravi.elixir-formatter"
    ]
}